{
  "compilerOptions": {
    "incremental": true,
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["@cloudflare/workers-types"],
    "lib": ["ESNext"],
    "jsx": "react",
    "outDir": "./dist",
    "noEmit": true,
    "sourceMap": true
  }
}
